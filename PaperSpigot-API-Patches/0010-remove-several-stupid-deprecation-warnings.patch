@@ -626,7 +626,6 @@ public interface Server extends PluginMessageRecipient {
      * @return an offline player
      * @see #getOfflinePlayer(java.util.UUID)
      */
-    @Deprecated
     public OfflinePlayer getOfflinePlayer(String name);
 
     /**
