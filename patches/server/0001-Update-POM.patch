+        String updateMessage = getUpdateStatusMessage("Alphheim/AlphheimCraft", GITHUB_BRANCH_NAME, parts[0]); // Alphheim
         String history = getHistory();
 
         return history != null ? history + "\n" + updateMessage : updateMessage;
