From 157d0f042e6443c152c7adb064fd0662faee95a7 Mon Sep 17 00:00:00 2001
From: Shane Freeder <theboyetronic@gmail.com>
Date: Fri, 11 May 2018 01:46:27 +0100
Subject: [PATCH] remove several stupid deprecation warnings

---
 src/main/java/org/bukkit/craftbukkit/CraftServer.java        | 4 ----
 src/main/java/org/bukkit/craftbukkit/entity/CraftPlayer.java | 1 -
 2 files changed, 5 deletions(-)

diff --git a/src/main/java/org/bukkit/craftbukkit/CraftServer.java b/src/main/java/org/bukkit/craftbukkit/CraftServer.java
index b18075b3f..c0cf1ab59 100644
--- a/src/main/java/org/bukkit/craftbukkit/CraftServer.java
+++ b/src/main/java/org/bukkit/craftbukkit/CraftServer.java
@@ -464,7 +464,6 @@ public final class CraftServer implements Server {
     }
 
     @Override
-    @Deprecated
     public Player getPlayer(final String name) {
         Validate.notNull(name, "Name cannot be null");
 
@@ -490,7 +489,6 @@ public final class CraftServer implements Server {
     }
 
     @Override
-    @Deprecated
     public Player getPlayerExact(String name) {
         Validate.notNull(name, "Name cannot be null");
 
@@ -519,7 +517,6 @@ public final class CraftServer implements Server {
     }
 
     @Override
-    @Deprecated
     public List<Player> matchPlayer(String partialName) {
         Validate.notNull(partialName, "PartialName cannot be null");
 
@@ -1413,7 +1410,6 @@ public final class CraftServer implements Server {
     // Paper end
 
     @Override
-    @Deprecated
     public OfflinePlayer getOfflinePlayer(String name) {
         Validate.notNull(name, "Name cannot be null");
         com.google.common.base.Preconditions.checkArgument( !org.apache.commons.lang.StringUtils.isBlank( name ), "Name cannot be blank" ); // Spigot
diff --git a/src/main/java/org/bukkit/craftbukkit/entity/CraftPlayer.java b/src/main/java/org/bukkit/craftbukkit/entity/CraftPlayer.java
index 96232aa07..eb228ed42 100644
--- a/src/main/java/org/bukkit/craftbukkit/entity/CraftPlayer.java
+++ b/src/main/java/org/bukkit/craftbukkit/entity/CraftPlayer.java
@@ -794,7 +794,6 @@ public class CraftPlayer extends CraftHumanEntity implements Player {
         server.getHandle().playerFileData.save(getHandle());
     }
 
-    @Deprecated
     @Override
     public void updateInventory() {
         getHandle().updateInventory(getHandle().activeContainer);
-- 
